{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ChatGPTConfig": {
        "InitialSystemMessage": null,
        "InitialUserMessage": null,
        "MaxTokens": null,
        "Model": null,
        "Temperature": null,
        "PassUserIdToOpenAiRequests": true
    },
    "Sentry": {
        "EnableTracing": true,
        "MaxRequestBodySize": "Always",
        "SendDefaultPii": true,
        "IncludeActivityData": true,
        "AttachStackTrace": true,
        "Debug": true,
        "DiagnosticLevel": "Error"
    }
}
